{
  "manifest_version": 2,
  "name": "formpie",
  "description": "copy form content then paste to another.",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}
